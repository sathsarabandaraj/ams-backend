{
  "name": "ams-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts --ignore-pattern /src/migrations",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "debug": "nodemon --watch 'src/**/*.ts' --exec ts-node-dev  --inspect --respawn src/server.ts",
    "typeorm:db": "npm run build && npx typeorm -d dist/src/configs/dbConfig.js",
    "migration:generate": "npm run typeorm:db -- migration:generate",
    "migration:run": "npm run typeorm:db -- migration:run",
    "migration:revert": "npm run typeorm:db -- migration:revert",
    "sync:db": "npm run typeorm:db schema:sync",
    "seed": "npx ts-node src/scripts/seed-db.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.9.0",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.2"
  }
}
