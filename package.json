{
  "name": "ams-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts --ignore-pattern /src/migrations",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "debug": "nodemon --watch 'src/**/*.ts' --exec ts-node-dev  --inspect --respawn src/server.ts",
    "typeorm:db": "npm run build && npx typeorm -d dist/src/configs/dbConfig.js",
    "migration:generate": "npm run typeorm:db -- migration:generate",
    "migration:run": "npm run typeorm:db -- migration:run",
    "migration:revert": "npm run typeorm:db -- migration:revert",
    "sync:db": "npm run typeorm:db schema:sync",
    "seed": "npx ts-node src/scripts/seed-db.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.710.0",
    "@types/aws-sdk": "^0.0.42",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/passport-local": "^1.0.38",
    "@types/redis": "^4.0.10",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "express": "^4.17.2",
    "i18next": "^23.16.5",
    "i18next-fs-backend": "^2.3.2",
    "i18next-http-middleware": "^3.6.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "winston": "^3.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.2"
  }
}
